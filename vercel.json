{
  "build": {
    "env": {
      "ENABLE_VC_BUILD": "1"
    }
  },
  "builds": [
    {
      "src": "./vercel/output/",
      "use": "@vercel/static"
    }
  ],
  "github": {
    "enabled": false
  },
  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=86400, public"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/resume",
      "destination": "http://resume.arkinmodi.com/"
    },
    {
      "source": "/github",
      "destination": "https://www.github.com/arkinmodi/"
    },
    {
      "source": "/linkedin",
      "destination": "https://www.linkedin.com/in/arkinmodi/"
    }
  ]
}